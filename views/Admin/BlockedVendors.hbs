
<div id="layoutSidenav_content">
                <main>
                    <div class="container-fluid">
                         {{!-- <h1 class="mt-1">Vendors</h1> --}}
                          <ol class="breadcrumb mb-4">
                            <li class="breadcrumb-item active">Vendors</li>
                        </ol>
  <ul class="nav nav-tabs">
  <li class="nav-item">
    <a class="nav-link" href="/admin/vendors">Vendors</a>
  </li>
  <li class="nav-item">
    <a class="nav-link" href="/admin/addVendor">Add Vendor</a>
  </li>
  <li class="nav-item">
    <a class="nav-link active" href="#">Blocked Vendors</a>
  </li>
</ul>
<div class="row" style="height:55px!important">
  {{#if SuccessMsg}}

                  <div class="alert alert-success" style="width: 100%;">
                   <strong> {{SuccessMsg_Caption}}</strong> {{SuccessMsg}}
                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>
                   </div>
                   {{/if}}
 </div>

<div class="row   border border-info">
   <h3>vendors</h3>
 <table class="table p-3" style="background-color:#e9ecef ;">
    
            <thead>
                <tr>
                  <th scope="col">Name</th>
                  {{!-- <th scope="col">Category</th> --}}
                   <th scope="col">Contact Person</th>
                    <th scope="col">Contact No</th>
                     <th scope="col">EmailId</th>
                  <th scope="col" style="width:10%">Description</th>
                  <th scope="col">Address</th>
                    <th scope="col">City</th>
                    <th scope="col">District</th>
                  <th scope="col">Action</th>
                </tr>
            </thead>
            <tbody>
                {{#each vendors}}
                <tr>
                    <td>{{this.name}}</td>
                    {{!-- <td>{{this.category}}</td> --}}
                      <td>{{this.contactPerson}}</td>
                    <td>{{this.phone}}</td>
                      <td>{{this.email}}</td>
                    <td style="width:10%">{{this.Description}}</td>
                    <td style="width:10%">{{this.Address}}</td>
                    <td>{{this.city}}</td>
                    <td>{{this.district}}</td>
                   <td>
                      <a  class="btn btn-primary" href="/admin/unblock-vendor/{{this._id}}" >UnBlock</a>
                      {{!-- <a  class="btn btn-primary" href="/admin/edit-vendor/{{this._id}}" >Edit</a> --}}
                        {{!-- <a  class="btn btn-danger" href="/admin/delete-vendor/{{this._id}}" onclick="return confirm('Are you sure you want to delete {{this.name}}?')">Delete</a> --}}
                    </td>
                </tr>
                {{/each}}
            </tbody>
        </table>
</div>
                    </div>
                </main>
        </div>

  